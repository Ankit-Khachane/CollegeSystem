<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="main.collegesystem.admin.atndnsdetail"
    tools:showIn="@layout/atndnsdetail_activity"
    android:background="#85a7ce">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name :"
        android:id="@+id/textView21"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Email :"
        android:id="@+id/textView22"
        android:layout_below="@+id/textView21"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/nameval"
        android:layout_alignTop="@+id/textView21"
        android:layout_toRightOf="@+id/textView21"
        android:layout_toEndOf="@+id/textView21"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:textSize="23dp"
        android:typeface="monospace"
        android:textColor="#000000" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mailval"
        android:textSize="23dp"
        android:typeface="monospace"
        android:layout_alignTop="@+id/textView22"
        android:layout_toEndOf="@+id/textView28"
        android:autoLink="email"
        android:layout_toRightOf="@+id/textView28"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Phone :"
        android:id="@+id/textView26"
        android:layout_below="@+id/textView22"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="33dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/phonetv"
        android:layout_alignTop="@+id/textView26"
        android:textSize="23dp"
        android:typeface="monospace"
        android:autoLink="phone"
        android:layout_alignLeft="@+id/branchtv"
        android:layout_alignStart="@+id/branchtv"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Branch :"
        android:id="@+id/textView28"
        android:layout_below="@+id/phonetv"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="15dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/branchtv"
        android:layout_alignTop="@+id/textView28"
        android:textSize="23dp"
        android:typeface="monospace"
        android:layout_toRightOf="@+id/textView29"
        android:layout_toEndOf="@+id/textView29"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Address :"
        android:id="@+id/textView29"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/branchtv" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/adrestv"
        android:layout_alignTop="@+id/textView29"
        android:lines="4"
        android:textSize="23dp"
        android:typeface="monospace"
        android:textColor="#000000"
        android:layout_alignLeft="@+id/nameval"
        android:layout_alignStart="@+id/nameval" />

</RelativeLayout>
